<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="cdav-connector_CNET">
		<class>it.cnet.connection.pojo.AdCaldavparam</class>
		<class>it.cnet.connection.pojo.AdUser</class>
		<class>it.cnet.connection.pojo.RRequest</class>
		<class>it.cnet.connection.pojo.RRequesttype</class>
		<class>it.cnet.connection.pojo.AdSysconfig</class>
		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:postgresql:idempiere"/>
			<property name="javax.persistence.jdbc.user" value="adempiere"/>
			<property name="javax.persistence.jdbc.password" value="adempiere"/>
			<property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
			<property name="eclipselink.allow-zero-id" value="true" />
		</properties>
	</persistence-unit>
</persistence>
